<h1>会員情報編集</h1>

<%= form_with model: @customer,url: customers_path do |f| %>

    <%= f.label :名前 %>
    
    <%= f.label :姓 %>
    <%= f.text_field :first_name %>
    
    <%= f.label :名 %>
    <%= f.text_field :last_name %>
    
    
    <%= f.label :フリガナ %>
    
    <%= f.label :セイ %>
    <%= f.text_field :first_name_kana %>
    
    <%= f.label :メイ %>
    <%= f.text_field :last_name_kana %>
    
    <%= f.label :郵便番号 %>
    <%= f.text_field :postal_code %>
  
    <%= f.label :住所 %>
    <%= f.text_field :address %>
  
    <%= f.label :電話番号 %>
    <%= f.text_field :telephone_number %>
    
    <%= f.label :メールアドレス %>
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>


  
  　<%= f.submit '編集内容を保存' %>
  　<%= link_to '退会する', customers_unsubscribe_path %>

  
  
  
<% end %>
