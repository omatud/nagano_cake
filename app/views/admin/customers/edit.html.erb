   <div class="container">
     <%= @customer.first_name %>
     <%= @customer.last_name %>さんの会員情報編集
     <div class="row pt-4">
       <div class="col">
         会員ID 
       </div>
       <div class="col">
         <%= @customer.id %>

       </div>
     </div>
     
     <%= form_with model: @customer,url: admin_customer_path,method: :patch do |f| %>
     
     <div class="row pt-4">
       <div class="col">
         <%= f.label :名前 %>
       </div>
       <div class="col">
         <%= f.text_field :first_name %>
         <%= f.text_field :last_name %>
       </div>
     </div>
     <div class="row pt-4">
       <div class="col">
         <%= f.label :フリガナ %>
       </div>
       <div class="col">
         <%= f.text_field :first_name_kana %>
         <%= f.text_field :last_name_kana %>
       </div>
     </div>
     <div class="row pt-4">
       <div class="col">
         <%= f.label :郵便番号 %>
       </div>
       <div class="col">
         <%= f.text_field :postal_code %>
       </div>
     </div>
     <div class="row pt-4">
       <div class="col">
         <%= f.label :住所 %>
       </div>
       <div class="col">
         <%= f.text_field :address %>
       </div>
     </div>
     <div class="row pt-4">
       <div class="col">
         <%= f.label :電話番号 %>
       </div>
       <div class="col">
         <%= f.text_field :telephone_number %>
       </div>
     </div>
     <div class="row pt-4">
       <div class="col">
         <%= f.label :メールアドレス %>
       </div>
       <div class="col">
         <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
       </div>
     </div>
     <div class="row pt-4">
       <div class="col">
         会員ステータス 
       </div>
       <div class="col">
         <%= f.radio_button :is_deleted,false %>有効
         <%= f.radio_button :is_deleted,true %>退会
       </div>
     </div>
     <div class="row pt-4">
       <div class="col text-center">
         <%= f.submit '変更を保存' %>
       </div>
       <% end %>
     </div>
   </div>
